<%= form_with model: @dog, local: true do |form| %>
    <%= form.label :name, '名前' %>
    <%= form.text_field :name %><br>
  
    <%= form.label :description, '説明' %>
    <%= form.text_area :description %><br>

    <%= form.label :age, '年齢' %>
    <%= form.text_area :age %><br>

    <%= form.label :breed, '内容' %>
    <%= form.text_area :breed %><br>
  
    <%= form.label :image, '画像URL' %>
    <%= form.text_field :image %><br>
  
    <%= form.submit %>
  <% end %>
  